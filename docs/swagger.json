{
    "openapi": "3.0.2",
    "info": {
        "title": "Games-API",
        "version": "1.0.0",
        "description": "### This is a Swagger UI for Games API"
    },
    "paths": {
        "/games": {
            "get": {
                "tags": [
                    "games"
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "summary": "Get a list of all games",
                "description": "Returns a list of all games in API"
            }
        }
    },
    "components": {
        "responses": {
            "ListOfGames": {
                "description": "{\n    \"Witcher 3\",\n    \"Cyberpunk 2077\";\n    \"Parrots on the moon 2\"\n}"
            }
        }
    }
}